    ArrUNIChars_Len2(100) = ChrW$(7902)
    ArrUNIChars_Len2(101) = ChrW$(7904)
    ArrUNIChars_Len2(102) = ChrW$(7906)
    ArrUNIChars_Len2(103) = ChrW$(218)
    ArrUNIChars_Len2(104) = ChrW$(217)
    ArrUNIChars_Len2(105) = ChrW$(7910)
    ArrUNIChars_Len2(106) = ChrW$(360)
    ArrUNIChars_Len2(107) = ChrW$(7908)
    ArrUNIChars_Len2(108) = ChrW$(7912)
