    ArrTCVN3Chars_Len2(96) = "ç"
    ArrTCVN3Chars_Len2(97) = "é"
    ArrTCVN3Chars_Len2(98) = "í"
    ArrTCVN3Chars_Len2(99) = "ê"
    ArrTCVN3Chars_Len2(100) = "ë"
    ArrTCVN3Chars_Len2(101) = "ì"
    ArrTCVN3Chars_Len2(102) = "î"
    ArrTCVN3Chars_Len2(103) = "ó"
    ArrTCVN3Chars_Len2(104) = "ï"
    ArrTCVN3Chars_Len2(105) = "ñ"
    ArrTCVN3Chars_Len2(106) = "ò"
    ArrTCVN3Chars_Len2(107) = "ô"
    ArrTCVN3Chars_Len2(108) = "ø"
    ArrTCVN3Chars_Len2(109) = "õ"
    ArrTCVN3Chars_Len2(110) = "ö"
    ArrTCVN3Chars_Len2(111) = "÷"
    ArrTCVN3Chars_Len2(112) = "ù"
    ArrTCVN3Chars_Len2(113) = "ý"
    ArrTCVN3Chars_Len2(114) = "ú"
    ArrTCVN3Chars_Len2(115) = "û"
    ArrTCVN3Chars_Len2(116) = "ü"

    Dim ArrVNIChars_Len2_TuyChinh(18) As String
    Dim ArrTCVN3Chars_Len2_TuyChinh(18) As String

    ArrVNIChars_Len2_TuyChinh(1) = "ô#"
    ArrVNIChars_Len2_TuyChinh(2) = "í#"
    ArrVNIChars_Len2_TuyChinh(3) = "ì#"
    ArrVNIChars_Len2_TuyChinh(4) = "æ#"
    ArrVNIChars_Len2_TuyChinh(5) = "ó#"
    ArrVNIChars_Len2_TuyChinh(6) = "ò#"
    ArrVNIChars_Len2_TuyChinh(7) = "ö#"
    ArrVNIChars_Len2_TuyChinh(8) = "î#"
    ArrVNIChars_Len2_TuyChinh(9) = "ñ#"
    ArrVNIChars_Len2_TuyChinh(10) = "Ô#"
    ArrVNIChars_Len2_TuyChinh(11) = "Í#"
    ArrVNIChars_Len2_TuyChinh(12) = "Ì#"
    ArrVNIChars_Len2_TuyChinh(13) = "Æ#"
    ArrVNIChars_Len2_TuyChinh(14) = "Ó#"
    ArrVNIChars_Len2_TuyChinh(15) = "Ò#"
    ArrVNIChars_Len2_TuyChinh(16) = "Ö#"
    ArrVNIChars_Len2_TuyChinh(17) = "Î#"
    ArrVNIChars_Len2_TuyChinh(18) = "Ñ#"

    ArrTCVN3Chars_Len2_TuyChinh(1) = "¬"
    ArrTCVN3Chars_Len2_TuyChinh(2) = "Ý"
    ArrTCVN3Chars_Len2_TuyChinh(3) = "×"
    ArrTCVN3Chars_Len2_TuyChinh(4) = "Ø"
    ArrTCVN3Chars_Len2_TuyChinh(5) = "Ü"
    ArrTCVN3Chars_Len2_TuyChinh(6) = "Þ"
    ArrTCVN3Chars_Len2_TuyChinh(7) = "­"
    ArrTCVN3Chars_Len2_TuyChinh(8) = "þ"
    ArrTCVN3Chars_Len2_TuyChinh(9) = "®"
    ArrTCVN3Chars_Len2_TuyChinh(10) = "¥"
    ArrTCVN3Chars_Len2_TuyChinh(11) = "Ý"
    ArrTCVN3Chars_Len2_TuyChinh(12) = "×"
    ArrTCVN3Chars_Len2_TuyChinh(13) = "Ø"
    ArrTCVN3Chars_Len2_TuyChinh(14) = "Ü"
    ArrTCVN3Chars_Len2_TuyChinh(15) = "Þ"
    ArrTCVN3Chars_Len2_TuyChinh(16) = "¦"
    ArrTCVN3Chars_Len2_TuyChinh(17) = "þ"
    ArrTCVN3Chars_Len2_TuyChinh(18) = "§"

    ''''

    sDuLieu = Replace(sDuLieu, "ô", "ô#")
    sDuLieu = Replace(sDuLieu, "í", "í#")
    sDuLieu = Replace(sDuLieu, "ì", "ì#")
    sDuLieu = Replace(sDuLieu, "æ", "æ#")
    sDuLieu = Replace(sDuLieu, "ó", "ó#")
    sDuLieu = Replace(sDuLieu, "ò", "ò#")
    sDuLieu = Replace(sDuLieu, "ö", "ö#")
    sDuLieu = Replace(sDuLieu, "î", "î#")
    sDuLieu = Replace(sDuLieu, "ñ", "ñ#")
    sDuLieu = Replace(sDuLieu, "Ô", "Ô#")
    sDuLieu = Replace(sDuLieu, "Í", "Í#")
    sDuLieu = Replace(sDuLieu, "Ì", "Ì#")
    sDuLieu = Replace(sDuLieu, "Æ", "Æ#")
    sDuLieu = Replace(sDuLieu, "Ó", "Ó#")
    sDuLieu = Replace(sDuLieu, "Ò", "Ò#")
    sDuLieu = Replace(sDuLieu, "Ö", "Ö#")
    sDuLieu = Replace(sDuLieu, "Î", "Î#")
    sDuLieu = Replace(sDuLieu, "Ñ", "Ñ#")

    ''''

    sDuLieu = Replace(sDuLieu, "ô#ù", "ôù")
    sDuLieu = Replace(sDuLieu, "ô#ø", "ôø")
    sDuLieu = Replace(sDuLieu, "ô#û", "ôû")
    sDuLieu = Replace(sDuLieu, "ô#õ", "ôõ")
    sDuLieu = Replace(sDuLieu, "ô#ï", "ôï")
    sDuLieu = Replace(sDuLieu, "ö#ù", "öù")
    sDuLieu = Replace(sDuLieu, "ö#ø", "öø")
    sDuLieu = Replace(sDuLieu, "ö#û", "öû")
    sDuLieu = Replace(sDuLieu, "ö#õ", "öõ")
    sDuLieu = Replace(sDuLieu, "ö#ï", "öï")
    sDuLieu = Replace(sDuLieu, "Ô#Ù", "ÔÙ")
    sDuLieu = Replace(sDuLieu, "Ô#Ø", "ÔØ")
    sDuLieu = Replace(sDuLieu, "Ô#Û", "ÔÛ")
    sDuLieu = Replace(sDuLieu, "Ô#Õ", "ÔÕ")
    sDuLieu = Replace(sDuLieu, "Ô#Ï", "ÔÏ")
    sDuLieu = Replace(sDuLieu, "Ö#Ù", "ÖÙ")
    sDuLieu = Replace(sDuLieu, "Ö#Ø", "ÖØ")
    sDuLieu = Replace(sDuLieu, "Ö#Û", "ÖÛ")
    sDuLieu = Replace(sDuLieu, "Ö#Õ", "ÖÕ")
    sDuLieu = Replace(sDuLieu, "Ö#Ï", "ÖÏ")

    Dim I As Integer

    For I = 1 To UBound(ArrVNIChars_Len2)
        sDuLieu = Replace(sDuLieu, ArrVNIChars_Len2(I), ArrTCVN3Chars_Len2(I))
    Next I

    For I = 1 To UBound(ArrVNIChars_Len2_TuyChinh)
        sDuLieu = Replace(sDuLieu, ArrVNIChars_Len2_TuyChinh(I), ArrTCVN3Chars_Len2_TuyChinh(I))
    Next I

    VMK_CONVERT_VNI_TO_TCVN3 = sDuLieu
End Function

Public Function VMK_CONVERT_VNI_TO_UNICODE(sDuLieu As String) As String

    Dim ArrVNIChars_Len2(116) As String
    Dim ArrUNIChars_Len2(116) As String

    ArrVNIChars_Len2(1) = "aù"
    ArrVNIChars_Len2(2) = "aø"
    ArrVNIChars_Len2(3) = "aû"
    ArrVNIChars_Len2(4) = "aõ"
    ArrVNIChars_Len2(5) = "aï"
    ArrVNIChars_Len2(6) = "aê"
    ArrVNIChars_Len2(7) = "aé"
    ArrVNIChars_Len2(8) = "aè"
    ArrVNIChars_Len2(9) = "aú"
    ArrVNIChars_Len2(10) = "aü"
    ArrVNIChars_Len2(11) = "aë"
    ArrVNIChars_Len2(12) = "aâ"
    ArrVNIChars_Len2(13) = "aá"
    ArrVNIChars_Len2(14) = "aà"
    ArrVNIChars_Len2(15) = "aå"
    ArrVNIChars_Len2(16) = "aã"
    ArrVNIChars_Len2(17) = "aä"
    ArrVNIChars_Len2(18) = "eù"
    ArrVNIChars_Len2(19) = "eø"
    ArrVNIChars_Len2(20) = "eû"
    ArrVNIChars_Len2(21) = "eõ"
