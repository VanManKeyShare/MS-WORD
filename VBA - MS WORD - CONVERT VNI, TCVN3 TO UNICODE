
VBA - MS WORD - CONVERT VNI, TCVN3 TO UNICODE

Option Explicit

Public Sub MsWord_CONVERT_VNI_TCVN3_TO_UNICODE()
    Dim sRange, sWord
    For Each sRange In ActiveDocument.StoryRanges
        For Each sWord In sRange.Words
            If Trim(sWord.Text) <> "" And Trim(sWord.Text) <> vbCr Then
                If Mid(LCase(sWord.Font.Name), 1, 3) = ".vn" Then
                    sWord.Text = VMK_CONVERT_TCVN3_TO_UNICODE(sWord.Text)
                    sWord.Font.Name = "Times New Roman"
                ElseIf Mid(LCase(sWord.Font.Name), 1, 3) = "vni" Then
                    sWord.Text = VMK_CONVERT_VNI_TO_UNICODE(sWord.Text)
                    sWord.Font.Name = "Times New Roman"
                End If
            End If
        Next
    Next
    MsgBox "Success", , "Thông Báo"
End Sub

Public Function VMK_CONVERT_TCVN3_TO_UNICODE(sDuLieu As String) As String
    Dim ArrUNIChars() As String: ArrUNIChars = VMK_Array_Unicode_Chars
    Dim ArrTCVN3Chars() As String: ArrTCVN3Chars = VMK_Array_TCVN3_Chars
    Dim I As Integer, sIndex As Integer
    Dim sCurr As String, Kq As String: Kq = ""
    For I = 1 To Len(sDuLieu)
        sCurr = Mid(sDuLieu, I, 1)
        sIndex = VMK_GET_INDEX_OF_ARRAY(ArrTCVN3Chars, sCurr)
        If sIndex > 0 Then sCurr = ArrUNIChars(sIndex)
        Kq = Kq + sCurr
    Next I
    VMK_CONVERT_TCVN3_TO_UNICODE = Kq
End Function

Public Function VMK_CONVERT_TCVN3_TO_VNI(sDuLieu As String) As String
    Dim ArrVNIChars() As String: ArrVNIChars = VMK_Array_VNI_Chars
    Dim ArrTCVN3Chars() As String: ArrTCVN3Chars = VMK_Array_TCVN3_Chars
    Dim I As Integer, sIndex As Integer
    Dim sCurr As String, Kq As String: Kq = ""
    For I = 1 To Len(sDuLieu)
        sCurr = Mid(sDuLieu, I, 1)
        sIndex = VMK_GET_INDEX_OF_ARRAY(ArrTCVN3Chars, sCurr)
        If sIndex > 0 Then sCurr = ArrVNIChars(sIndex)
        Kq = Kq + sCurr
    Next I
    VMK_CONVERT_TCVN3_TO_VNI = Kq
End Function

Public Function VMK_CONVERT_UNICODE_TO_TCVN3(sDuLieu As String) As String
    Dim ArrUNIChars() As String: ArrUNIChars = VMK_Array_Unicode_Chars
    Dim ArrTCVN3Chars() As String: ArrTCVN3Chars = VMK_Array_TCVN3_Chars
    Dim I As Integer, sIndex As Integer
    Dim sCurr As String, Kq As String: Kq = ""
    For I = 1 To Len(sDuLieu)
        sCurr = Mid(sDuLieu, I, 1)
        sIndex = VMK_GET_INDEX_OF_ARRAY(ArrUNIChars, sCurr)
        If sIndex > 0 Then sCurr = ArrTCVN3Chars(sIndex)
        Kq = Kq + sCurr
    Next I
    VMK_CONVERT_UNICODE_TO_TCVN3 = Kq
End Function

Public Function VMK_CONVERT_UNICODE_TO_VNI(sDuLieu As String) As String
    Dim ArrVNIChars() As String: ArrVNIChars = VMK_Array_VNI_Chars
    Dim ArrUNIChars() As String: ArrUNIChars = VMK_Array_Unicode_Chars
    Dim I As Integer, sIndex As Integer
    Dim sCurr As String, Kq As String: Kq = ""
    For I = 1 To Len(sDuLieu)
        sCurr = Mid(sDuLieu, I, 1)
        sIndex = VMK_GET_INDEX_OF_ARRAY(ArrUNIChars, sCurr)
        If sIndex > 0 Then sCurr = ArrVNIChars(sIndex)
        Kq = Kq + sCurr
    Next I
    VMK_CONVERT_UNICODE_TO_VNI = Kq
End Function

