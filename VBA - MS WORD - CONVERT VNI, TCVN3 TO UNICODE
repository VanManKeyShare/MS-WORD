    ArrUNIChars_Len2(114) = ChrW$(7922)
    ArrUNIChars_Len2(115) = ChrW$(7926)
    ArrUNIChars_Len2(116) = ChrW$(7928)

    Dim ArrVNIChars_Len2_TuyChinh(6) As String
    Dim ArrUNIChars_Len2_TuyChinh(6) As String

    ArrVNIChars_Len2_TuyChinh(1) = "ô#"
    ArrVNIChars_Len2_TuyChinh(2) = "ó#"
    ArrVNIChars_Len2_TuyChinh(3) = "ò#"
    ArrVNIChars_Len2_TuyChinh(4) = "Ô#"
    ArrVNIChars_Len2_TuyChinh(5) = "Ó#"
    ArrVNIChars_Len2_TuyChinh(6) = "Ò#"

    ArrUNIChars_Len2_TuyChinh(1) = ChrW$(417)
    ArrUNIChars_Len2_TuyChinh(2) = ChrW$(297)
    ArrUNIChars_Len2_TuyChinh(3) = ChrW$(7883)
    ArrUNIChars_Len2_TuyChinh(4) = ChrW$(416)
    ArrUNIChars_Len2_TuyChinh(5) = ChrW$(296)
    ArrUNIChars_Len2_TuyChinh(6) = ChrW$(7882)

    ''''

    sDuLieu = Replace(sDuLieu, "ô", "ô#")
    sDuLieu = Replace(sDuLieu, "ó", "ó#")
    sDuLieu = Replace(sDuLieu, "ò", "ò#")
    sDuLieu = Replace(sDuLieu, "Ô", "Ô#")
    sDuLieu = Replace(sDuLieu, "Ó", "Ó#")
    sDuLieu = Replace(sDuLieu, "Ò", "Ò#")

    ''''

    sDuLieu = Replace(sDuLieu, "ô#ù", "ôù")
    sDuLieu = Replace(sDuLieu, "ô#ø", "ôø")
    sDuLieu = Replace(sDuLieu, "ô#û", "ôû")
    sDuLieu = Replace(sDuLieu, "ô#õ", "ôõ")
    sDuLieu = Replace(sDuLieu, "ô#ï", "ôï")

    sDuLieu = Replace(sDuLieu, "Ô#Ù", "ÔÙ")
    sDuLieu = Replace(sDuLieu, "Ô#Ø", "ÔØ")
    sDuLieu = Replace(sDuLieu, "Ô#Û", "ÔÛ")
    sDuLieu = Replace(sDuLieu, "Ô#Õ", "ÔÕ")
    sDuLieu = Replace(sDuLieu, "Ô#Ï", "ÔÏ")

    Dim I As Integer
    Dim sCurr As String, Kq As String: Kq = ""

    For I = 1 To UBound(ArrVNIChars_Len2)
        sDuLieu = Replace(sDuLieu, ArrVNIChars_Len2(I), ArrUNIChars_Len2(I))
    Next I

    For I = 1 To UBound(ArrVNIChars_Len2_TuyChinh)
        sDuLieu = Replace(sDuLieu, ArrVNIChars_Len2_TuyChinh(I), ArrUNIChars_Len2_TuyChinh(I))
    Next I

    For I = 1 To Len(sDuLieu)
        sCurr = Mid(sDuLieu, I, 1)
        Select Case sCurr
            Case "æ": sCurr = ChrW$(7881)
            Case "ö": sCurr = ChrW$(432)
            Case "î": sCurr = ChrW$(7925)
            Case "ñ": sCurr = ChrW$(273)
            Case "Æ": sCurr = ChrW$(7880)
            Case "Ö": sCurr = ChrW$(431)
            Case "Î": sCurr = ChrW$(7924)
            Case "Ñ": sCurr = ChrW$(272)
        End Select
        Kq = Kq + sCurr
    Next I

    VMK_CONVERT_VNI_TO_UNICODE = Kq
End Function

Private Function VMK_GET_INDEX_OF_ARRAY(ArrSource() As String, zValue As String) As Integer
    Dim I As Integer
    Dim zIndex As Integer: zIndex = -1
    For I = 1 To UBound(ArrSource)
        If zValue = ArrSource(I) Then
            zIndex = I
            Exit For
        End If
    Next I
    VMK_GET_INDEX_OF_ARRAY = zIndex
End Function

