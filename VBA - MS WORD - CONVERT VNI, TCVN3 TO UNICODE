    ArrVNIChars(24) = "eï"
    ArrVNIChars(25) = "eâ"
    ArrVNIChars(26) = "eá"
    ArrVNIChars(27) = "eà"
    ArrVNIChars(28) = "eå"
    ArrVNIChars(29) = "eã"
