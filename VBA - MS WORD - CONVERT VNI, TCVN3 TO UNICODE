Private Function VMK_Array_VNI_Chars() As Variant
    Dim ArrVNIChars(146) As String
    ArrVNIChars(1) = "a"
    ArrVNIChars(2) = "aù"
    ArrVNIChars(3) = "aø"
    ArrVNIChars(4) = "aû"
    ArrVNIChars(5) = "aõ"
    ArrVNIChars(6) = "aï"
    ArrVNIChars(7) = "aê"
