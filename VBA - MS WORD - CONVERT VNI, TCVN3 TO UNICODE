    ArrUNIChars_Len2(19) = ChrW$(232)
    ArrUNIChars_Len2(20) = ChrW$(7867)
    ArrUNIChars_Len2(21) = ChrW$(7869)
    ArrUNIChars_Len2(22) = ChrW$(7865)
